<Window x:Class="Projekt_P4_Bookshop.Cart"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekt_P4_Bookshop"
        mc:Ignorable="d"
        Title="Cart" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="customer_name_cart" HorizontalAlignment="Left" Height="26" Margin="707,7,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="77" TextChanged="customer_name_cart_TextChanged" IsEnabled="False"/>
        <TextBox x:Name="customer_id_card" HorizontalAlignment="Left" Height="26" Margin="707,38,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="77" IsEnabled="False" TextChanged="customer_id_card_TextChanged"/>
        <Label Content="ID :" HorizontalAlignment="Left" Margin="664,38,0,0" VerticalAlignment="Top"/>
        <Label Content="Klient :" HorizontalAlignment="Left" Height="26" Margin="649,7,0,0" VerticalAlignment="Top" Width="53"/>
        <Button x:Name="button_return_cart" Content="Do Sklepu" HorizontalAlignment="Left" Height="54" Margin="543,10,0,0" VerticalAlignment="Top" Width="101" Click="button_return_cart_Click"/>

    </Grid>
</Window>
